pipeline {
    agent any

    environment{
        VERSION_NAME:"1.111"
    }

    stages{
        stage("compile"){
            steps{
                bat 'javac Test.java'
                echo "its env version name : ${VERSION_NAME}"
            }
        }

        stage("run"){
            steps{
                bat 'java Test'
            }
        }
    }

    post {
        always {
            echo "Always"
        }
        success {
            echo "its success"
        }
        failure {
            echo "its failure"
        }
    }
}